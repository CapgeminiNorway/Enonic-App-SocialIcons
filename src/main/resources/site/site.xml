<?xml version="1.0" encoding="UTF-8"?>
<site>
  <form>
  <item-set name="socialIcons"> 
  <label>Social Icons</label> 
  <occurrences minimum="0" maximum="0"/> 
  <items>
    <input name="url" type="TextLine">
      <label>URL</label>
       <help-text>Type in URL for Social Media </help-text>
      <occurrences minimum="1" maximum="1"/>
      <config>
        <regexp>https?:\/\/(www\.)?[-a-zA-Z0-9@:%._\+~#=]{1,256}\.[a-zA-Z0-9()]{1,6}\b([-a-zA-Z0-9()@:%_\+.~#?&amp;=]*)</regexp>
      </config>
    </input>
    <input name="font" type="TextLine">
      <label>Fontawesome tag</label>
    <help-text>Visit FontAwesome and get tags example: facebook-f</help-text>
      <occurrences minimum="1" maximum="1"/>
    </input>
    <input name="altText" type="TextLine">
      <label>AltText</label>
      <occurrences minimum="1" maximum="1"/>
    </input>
  </items>
</item-set>
  
  </form>
</site>
